{
    "etl": [
        {
            "name": "test-job",
            "extracts": [
                {
                    "name": "extract-drop-test",
                    "data_format": "csv",
                    "location": "tests/e2e/job/drop/input_data.csv",
                    "method": "batch",
                    "options": {
                        "delimiter": ",",
                        "header": true,
                        "inferSchema": false
                    },
                    "schema": "tests/e2e/job/drop/input_schema.json"
                }
            ],
            "transforms": [
                {
                    "name": "transform-drop-test",
                    "upstream_name": "extract-drop-test",
                    "options": {},
                    "functions": [
                        { "function": "drop", "arguments": { 
                                "columns": ["address", "phone", "temp_column"]
                            }
                        }
                    ]
                }
            ],
            "loads": [
                {
                    "name": "load-test",
                    "upstream_name": "transform-drop-test",
                    "data_format": "csv",
                    "location": "tests/e2e/job/drop/load-test",
                    "method": "batch",
                    "mode": "overwrite",
                    "options": {
                        "header": true
                    },
                    "schema_location": "tests/e2e/job/drop/schema_output.json"
                }
            ]
        }
    ],
    "alert": {
        "channels": [],
        "triggers": []
    }
}
