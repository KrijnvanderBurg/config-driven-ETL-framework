{
    "job": {
        "extracts": [
            {
                "data_format": "csv",
                "location": "tests/e2e/join/left_data.csv",
                "method": "batch",
                "name": "extract-left",
                "options": {
                    "delimiter": ",",
                    "header": true,
                    "inferSchema": false
                },
                "schema": "tests/e2e/join/left_schema.json"
            },
            {
                "data_format": "csv",
                "location": "tests/e2e/join/right_data.csv",
                "method": "batch",
                "name": "extract-right",
                "options": {
                    "delimiter": ",",
                    "header": true,
                    "inferSchema": false
                },
                "schema": "tests/e2e/join/right_schema.json"
            }
        ],
        "transforms": [
            {
                "name": "transform-join",
                "upstream_name": "extract-left",
                "functions": [
                    {
                        "function": "join",
                        "arguments": {
                            "other_upstream_name": "extract-right",
                            "on": "id",
                            "how": "inner"
                        }
                    },
                    {
                        "function": "select",
                        "arguments": {
                            "columns": ["id", "name", "category", "value"]
                        }
                    }
                ]
            }
        ],
        "loads": [
            {
                "name": "load-test",
                "upstream_name": "transform-join",
                "data_format": "csv",
                "location": "tests/e2e/join/load-test",
                "schema_location": "tests/e2e/join/schema_output.json",
                "method": "batch",
                "mode": "overwrite",
                "options": {
                    "header": true
                }
            }
        ]
    },
    "alert": {
        "channels": [],
        "triggers": []
    }
}
