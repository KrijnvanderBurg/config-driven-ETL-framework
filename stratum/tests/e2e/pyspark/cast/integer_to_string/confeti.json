{
    "engine": "pyspark",
    "extracts": [
        {
            "name": "extract-test",
            "method": "batch",
            "data_format": "csv",
            "location": "tests/e2e/pyspark/cast/integer_to_string/input_data.csv",
            "schema": "tests/e2e/pyspark/cast/integer_to_string/input_schema.json",
            "options": {
                "header": true,
                "inferSchema": false,
                "delimiter": ","
            }
        }
    ],
    "transforms": [
        {
            "name": "transform-test",
            "upstream_name": "extract-test",
            "functions": [
                {"function": "cast", "arguments": {"column_name": "age", "data_type": "string"}}
            ]
        }
    ],
    "loads": [
        {
            "name": "load-test",
            "upstream_name": "transform-test",
            "method": "batch",
            "data_format": "csv",
            "mode": "overwrite",
            "location": "tests/e2e/pyspark/cast/integer_to_string/csv",
            "schema_location": "tests/e2e/pyspark/cast/integer_to_string/schema_output.json",
            "options": {}
        }
    ]
}